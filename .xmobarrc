Config { alignSep = "}{",
         bgColor = "#263238",
         fgColor = "#ffffff",
         commands = [ Run BatteryP ["BAT0","BAT1","BAT2"]
                                   ["-t", "<acstatus> <left>% <timeleft>",
                                    "--",
                                    "-O", "+",
                                    "-i", "=",
                                    "-o", "-"]
                                   10,
                      Run MultiCpu ["-t", "CPU: <total>%",
                                    "--High", "85", "--high", "#cf6a4c"]
                                   10,
                      Run Memory ["--template", "Mem: <usedratio>%",
                                  "--High", "85", "--high", "#cf6a4c"]
                                 10,
                      Run Date "%d.%m.%Y %H:%M" "datetime" 600,
                      Run Kbd [],
                      Run StdinReader
                    ],
         template = "%StdinReader% } %datetime% { %kbd% <fc=#5f5a60>/</fc> %multicpu% <fc=#5f5a60>/</fc> %memory% <fc=#5f5a60>/</fc> %battery% <fc=#5f5a60></fc>"
       }
